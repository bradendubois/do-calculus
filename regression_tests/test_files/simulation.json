    }, {
      "name": "Probability Test: P(a~b~c)",
      "type": "probability",
      "args": ["A = a, B = ~b, C = ~c"],
      "expected_result": 0.162
    }, {
      "name": "Probability Test: P(~abc)",
      "type": "probability",
      "args": ["A = ~a, B = b, C = c"],
      "expected_result": 0.112
    }, {
      "name": "Probability Test: P(~ab~c)",
      "type": "probability",
      "args": ["A = ~a, B = b, C = ~c"],
      "expected_result": 0.084
    }, {
      "name": "Probability Test: P(~a~bc)",
      "type": "probability",
      "args": ["A = ~a, B = ~b, C = c"],
      "expected_result": 0.252
    }, {
      "name": "Probability Test: P(~a~b~c)",
      "type": "probability",
      "args": ["A = ~a, B = ~b, C = ~c"],
      "expected_result": 0.018
